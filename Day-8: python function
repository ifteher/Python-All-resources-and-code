#define
def double_it(num):
    result= num*2
    print("The double of", num, "is", result)
    return result
 
double_it(5)
def sum(num1,num2):
    result=num1+num2
    return result
total=sum(44, 45)
print("The sum of 44 and 45 is", total)
def multiply(num1, num2):
    result = num1 * num2
    print("The product of", num1, "and", num2, "is", result)
    return result
multiply(6, 7)
def divide(num1, num2):
    if num2 == 0:
        print("Cannot divide by zero")
        return None
    result = num1 / num2
    print("The division of", num1, "by", num2, "is", result)
    return result
final=double_it(total)
print("The final result after doubling the sum is", final)
divide(100, 5)

'''
#OUTPUT:
The double of 5 is 10
The sum of 44 and 45 is 89
The product of 6 and 7 is 42
The double of 89 is 178
The final result after doubling the sum is 178
The division of 100 by 5 is 20.0
'''
